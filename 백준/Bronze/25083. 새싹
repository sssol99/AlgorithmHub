import java.util.*;
import java.lang.*;
import java.io.*;

class Main {
    //, 와 ', / 는 제어문자가 아니다
    public static void main(String[] args) {
        System.out.println("         ,r'\"7");
        System.out.println("r`-_   ,'  ,/");
        System.out.println(" \\. \". L_r'");
        System.out.println("   `~\\/");
        System.out.println("      |");
        System.out.print("      |");
    }
